{
  "$schema": "../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../dist",
  "deleteDestPath": false,
  "whitelistedNonPeerDependencies": [
    "bowser",
    "date-fns",
    "element-closest",
    "extend",
    "is-ua-webview",
    "popper.js"
  ],
  "lib": {
    "entryFile": "public.ts",
      "umdModuleIds": {
          "bowser": "bowser",
          "date-fns": "date-fns",
          "date-fns/locale/en-US": "date-fns",
          "element-closest": "element-closest",
          "extend": "extend",
          "is-ua-webview": "is-ua-webview",
          "popper.js": "popper.js"
      }
  }
}
